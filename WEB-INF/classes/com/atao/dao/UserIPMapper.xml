<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atao.dao.UserIPMapper">

    <insert id="addUserIP" parameterType="String">
        insert into easyexchangemall.ip(address)
        value (#{ip})
    </insert>

</mapper>